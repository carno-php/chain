{
  "name": "carno-php/chain",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "carno-php/coroutine": "^1.0",
    "carno-php/promise": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7"
  },
  "autoload": {
    "psr-4": {
